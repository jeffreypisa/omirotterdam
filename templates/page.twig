{% extends "base.twig" %}

{% block content %}
	<article class="post-type-{{ post.post_type }}" id="post-{{ post.ID }}">
		{% if post.content %}
			<section>
				<div class="container">
					{{ post.content }}
				</div>
			</section>
		{% endif %}
		{% include 'pagebuilder.twig' %}
	</article>
{% endblock %}